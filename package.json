  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/umdm-plus.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/umdm-plus/issues"
  },
  "homepage": "https://github.com/yourusername/umdm-plus#readme",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
